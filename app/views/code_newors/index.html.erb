<p id="notice"><%= notice %></p>

<h1>Code Newors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Priority</th>
      <th>Content</th>
      <th>Ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_newors.each do |code_newor| %>
      <tr>
        <td><%= code_newor.name %></td>
        <td><%= code_newor.priority %></td>
        <td><%= code_newor.content %></td>
        <td><%= code_newor.ref %></td>
        <td><%= link_to 'Show', code_newor %></td>
        <td><%= link_to 'Edit', edit_code_newor_path(code_newor) %></td>
        <td><%= link_to 'Destroy', code_newor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code Newor', new_code_newor_path %>
